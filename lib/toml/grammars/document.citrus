grammar Document
  include Primitive
  include TomlArray

  rule document
    (comment | array_tables | keygroup | keyvalue | line_break)*
  end

  rule array_tables
    (space? '[[' (space? key space? "."?)+ ']]' space? comment?) <ArrayTables>
  end

  rule keygroup
    (space? '[' (space? key space? "."?)+ ']' space? comment?) <Keygroup>
  end

  rule keyvalue
    (space? key space? '=' space? v:(primitive | array) comment?) <Keyvalue>
  end
end
